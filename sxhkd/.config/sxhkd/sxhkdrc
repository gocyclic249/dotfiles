super + F2
    sh ~/bin/passmenu

super + shift + f
    firefox

super + shift + b
    qutebrowser

super + shift + m 
    st -t "ncmpcpp" -e ncmpcpp

super + F6
    xbacklight -dec 10

super + F7
    xbacklight -inc 10

super + F8
    amixer set Master toggle

super + F9
    amixer set Master 5%-

super + F10
    amixer set Master 5%+

XF86TouchpadOn
    dmenu_run

XF86Tools
    st -t vim -e vim

super + shift + w 
    st -t "weechat" -e weechat

super + shift + r 
    st -t "rtv" -e rtv
