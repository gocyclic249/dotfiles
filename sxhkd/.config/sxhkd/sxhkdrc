    amixer set Master 5%-

super + F10
    amixer set Master 5%+

XF86Launch5
    uxterm -e zsh -c "tmux -q has-session && exec tmux attach-session -d || exec tmux new-session -n$USER -s$USER@$HOSTNAME"

