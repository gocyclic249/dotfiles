@article{nicholson_new_2011,
	title = {New Technology, Old Issues: Demonstrating the Relevance of the Canadian Code of Ethics for Psychologists to the Ever-Sharper Cutting Edge of Technology},
	volume = {52},
	rights = {Copyright Canadian Psychological Association Aug 2011},
	issn = {07085591},
	url = {http://search.proquest.com.ezproxy2.apus.edu/docview/887728202/abstract},
	shorttitle = {New Technology, Old Issues},
	abstract = {One topic that has had considerable attention is online counselling, also known as e-therapy or telepsychology (e.g., Barnett \& Scheetz, 2003; Kanani \& Regehr, 2003; Marks \& Cavanagh, 2009; Rummell \& Joyce, 2010). However, many other topics can be found, such as computerized online test scoring services (Fisher \& Fried, 2003), e-mail teleconsultations (Bergus, Emerson, Reed, \& Attaluri, 2006; Car \& Sheikh, 2004), searching online for client information (i.e., "Googling your client"; Clinton, Silverman, \& Brendel, 2010), online client discussion groups (Humphreys, Winzelberg, \& Klaw, 2000), the use of online psychological assessments (Butcher, Perry, \& Hahn, 2004; Naglieri et al., 2004), the management of negative reviews published online through provider-rating sites (Luo \& Smith, 201 1), e-mail communications with clients (Gutheil \& Simon, 2005; Kassaw \& Gabbard, 2002), electronic health records (Iacovino, 2004; Jain \& Roberts, 2009; {McSherry}, 2004; Richards, 2009), cross-border regulatory considerations (Koocher \& Murray, 2000; Smith, 2010), online test sales and auctions (Lobello \& Zachar, 2007), the need to educate the public about the uneven quality of online health information (Goldner, 2006), online reimbursement and billing (Maheu, 2003; Ragusea \& {VandenCreek}, 2003), client blogs (Tunick \& Mednick, 2009), and online threats to test security ({CPA}, 2009; Ruiz, Drake, Glass, Marcotte \& van Gorp, 2002).
One of the more unique dilemmas relating to online social networking sites are the potential for a client to request to be a "friend" of the psychologist on such a site. Such "friending" of clients (which has a different definition for online "friend" than might be used in other interactions) would result in crossing traditional psychotherapy boundaries and change the frame of reference for the professional relationship (Luo, 2009). Even if one does not accept the "friend request" from a client, it does not stop the client from potentially having mutual "friends" on a social networking site. If this happens, the mutual friend may, for example, have photos of the psychologist on his or her website that are tagged with the names of the people in the photos. In fact, new photo equipment routinely "{GeoTag}" such photos when they were taken, giving exact {GPS} coordinates. Such "{GeoTagging}" allows information on the exact location of where the photo was taken to be a part of the photo's information and can be easily obtained by downloading the photo. In this and other ways, being a social network "friend of a friend" can lead to unintended and unwanted self-disclosures of personal information (e.g., where the psychologist lives).},
	pages = {215--224},
	number = {3},
	journaltitle = {Canadian Psychology},
	author = {Nicholson, Ian R.},
	urldate = {2016-01-23},
	date = {2011-08},
	keywords = {Advertising, Advertising, Information storage, Information storage, Internet, Internet, Professional ethics, Professional ethics, Psychology, Psychology, Social networks, Social networks},
	file = {Full Text PDF:/Users/eleventh/Library/Application Support/Zotero/Profiles/mc9qqt6b.default/zotero/storage/2P7MEWQQ/Nicholson - 2011 - New Technology, Old Issues Demonstrating the Rele.pdf:application/pdf}
}

